{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates" : {
        "title": "Vialer-js",
        "logo": {
            "url": "logo-64.png",
            "width": "72px",
            "height": "72px"
        },
        "name": "Vialer-js",
        "footerText": "&copy; Devhouse Spindle",
        "useCollapsibles": true,
        "tabNames": {
            "api": "Code",
            "tutorials": "Manuals"
        },
        "staticFiles": {
            "include": [
                "./src/img/logo-64.png"
            ]
        }
    },
    "opts": {
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/jsdoc-rtd",
        "tutorials": "./docs/manuals"
    }
}
